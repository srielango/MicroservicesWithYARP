{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "YARP": {
    "Routes": {
      "randomRoute": {
        "ClusterId": "randomCluster",
        "Match": {
          "Path": "/random"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/random"
          }
        ]
      },
      "catchall": {
        "ClusterId": "catchAllCluster",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "randomCluster": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "Blazor": {
            "Address": "https://www.iamtimcorey.com/courses/blazor-from-start-to-finish/"
          },
          "CSharpMasterCourse": {
            "Address": "https://www.iamtimcorey.com/courses/csharp-mastercourse/"
          },
          "GitHubAccelerate": {
            "Address": "https://www.iamtimcorey.com/courses/accelerate-github/"
          }
        }
      },
      "catchAllCluster": {
        "Destinations": {
          "IAmTimCorey": {
            "Address": "https://www.iamtimcorey.com/"
          }
        }
      }
    }
  }
}
