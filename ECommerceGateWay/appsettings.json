{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "YARP": {
    "Routes": {
      "weatherRoute": {
        "ClusterId": "catalogApiCluster",
        "Match": {
          "Path": "/catalog/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/catalog"
          }
        ]
      }
    },
    "Clusters": {
      "catalogApiCluster": {
        "Destinations": {
          "catalog": {
            "Address": "https://localhost:7001/"
          }
        }
      }
    }
  }
}
